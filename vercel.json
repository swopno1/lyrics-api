{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "app.js"
    },
    {
      "src": "/songs",
      "methods": ["GET", "POST"],
      "dest": "app.js"
    },
    {
      "src": "/songs/:id",
      "methods": ["GET", "PUT", "DELETE"],
      "dest": "app.js"
    },
    {
      "src": "/users",
      "methods": ["GET"],
      "dest": "app.js"
    },
    {
      "src": "/users/login",
      "methods": ["POST"],
      "dest": "app.js"
    },
    {
      "src": "/users/signup",
      "methods": ["POST"],
      "dest": "app.js"
    },
    {
      "src": "/users/update/:id",
      "methods": ["PUT"],
      "dest": "app.js"
    },
    {
      "src": "/api-docs",
      "dest": "swagger.js"
    }
  ]
}
